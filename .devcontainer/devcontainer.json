{
    "name": "workscreen",
    "dockerComposeFile": ["docker-compose.yml"],
    "service": "workscreen",
    "workspaceFolder": "/app",
    "extensions": [
        "ms-azuretools.vscode-docker"
    ],
    "shutdownAction": "stopCompose",
    "settings": {
        "remote.extensionKind": {
            "ms-azuretools.vscode-docker": "ui"
        },
        "files.associations": {
            "[Dd]ockerfile*": "dockerfile"
        }
    }
}